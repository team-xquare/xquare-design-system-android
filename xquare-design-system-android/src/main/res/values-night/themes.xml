<?xml version="1.0" encoding="utf-8"?><!--color 참조 시 @color 가 아닌 ?attr/ 로 참조하여 사용-->
<resources>

    <style name="Theme.XQUARE.DayNight.NoActionBar" parent="Theme.Material3.DynamicColors.DayNight">
        <!--primary-->
        <item name="colorPrimary">@color/primary_40</item>
        <item name="colorOnPrimary">@color/primary_100</item>
        <item name="colorPrimaryContainer">@color/primary_30</item>
        <item name="colorOnPrimaryContainer">@color/primary_90</item>

        <!--secondary-->
        <item name="colorSecondary">@color/secondary_40</item>
        <item name="colorOnSecondary">@color/secondary_100</item>
        <item name="colorSecondaryContainer">@color/secondary_30</item>
        <item name="colorOnSecondaryContainer">@color/secondary_90</item>

        <!--tertiary-->
        <item name="colorTertiary">@color/tertiary_40</item>
        <item name="colorOnTertiary">@color/tertiary_100</item>
        <item name="colorTertiaryContainer">@color/tertiary_30</item>
        <item name="colorOnTertiaryContainer">@color/tertiary_90</item>

        <!--error-->
        <item name="colorError">@color/error_40</item>
        <item name="colorOnError">@color/error_100</item>
        <item name="colorErrorContainer">@color/error_30</item>
        <item name="colorOnErrorContainer">@color/error_90</item>

        <!--background-->
        <item name="colorOnBackground">@color/neutral_90</item>

        <!--surface-->
        <item name="colorSurface">@color/neutral_0</item>
        <item name="colorOnSurface">@color/neutral_100</item>
        <item name="colorSurfaceVariant">@color/neutral_variant_30</item>
        <item name="colorOnSurfaceVariant">@color/neutral_variant_80</item>

        <!--Outline-->
        <item name="colorOutline">@color/neutral_variant_60</item>
        <item name="colorOutlineVariant">@color/neutral_variant_30</item>
    </style>
</resources>
